{
  "private": true,

     "name": "booth",
  "version": "0.15.0-pre",
  "license": "ISC",

  "author": "Strider <strange.transistor@gmail.com>",
  "description": "Booth provides symmetric Event API over websocket or unix domain socket",

  "homepage":            "https://github.com/StreetStrider/booth",
  "repository": { "url": "https://github.com/StreetStrider/booth.git", "type": "git" },
  "bugs":       { "url": "https://github.com/StreetStrider/booth/issues" },

  "engines": { "node": ">=8" },

  "files":
  [
    "index.js",

    "Booth.js",
    "Endpoint.js",

    "Addr.js",

    "Events.js",

    "midw/",
    "room/",

    "**/*.d.ts"
  ],

  "peerDependencies":
  {
    "ws":
      "6 || 7 || 8",

    "@types/ws":
      "*"
  },

  "peerDependenciesMeta":
  {
    "@types/ws": { "optional": true },
           "ws": { "optional": true }
  },

  "scripts":
  {
    "start": "node -r esm test/socket.test.js",

       "st": "eslint .",
     "unit": "test/run",
       "ts": "dtslint --expectOnly --localTs ./node_modules/typescript/lib",

     "test": "npm run st && npm run unit && npm run ts",

    "build": "gulp --final --to npm"
  },

  "dependencies":
  {
    "@streetstrider/emitter":
      "^1.2",

    "isomorphic-ws":
      "4",

    "aux.js":
      "^3.5"
  },

  "devDependencies":
  {
    "esm":
      "3",

    "console-ultimate":
      "3",

    "ws":
      "8",


    "gulp":
      "4",

    "rollup":
      "2",

    "metalpipe":
      "https://github.com/StrangeTransistor/metalpipe.git#bccfd822c84892bca6aa3ac9c489dd9f1db0dea1",


    "eslint":
      "7",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.10.0",

    "eslint-plugin-node":
      "11",


    "dtslint":
      "4.2",

    "typescript":
      "4.4.4",

    "@types/node":
      "16",

    "@types/ws":
      "^8.2",


    "chai":
      "4"
  }
}