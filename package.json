{
  "private": true,

     "name": "booth",
  "version": "0.16.0-pre",
  "license": "ISC",

  "author": "Strider <strange.transistor@gmail.com>",
  "description": "Booth provides symmetric Event API over websocket or unix domain socket",

  "homepage":            "https://github.com/StreetStrider/booth",
  "repository": { "url": "https://github.com/StreetStrider/booth.git", "type": "git" },
  "bugs":       { "url": "https://github.com/StreetStrider/booth/issues" },

  "engines": { "node": ">=8.3" },

  "files":
  [
    "index.js",

    "Booth.js",
    "Endpoint.js",

    "Addr.js",

    "Events.js",

    "midw/",
    "_/",

    "**/*.d.ts"
  ],

  "peerDependencies":
  {
    "ws":
      "6 || 7 || 8",

    "@types/ws":
      "*"
  },

  "peerDependenciesMeta":
  {
           "ws": { "optional": true },
    "@types/ws": { "optional": true }
  },

  "scripts":
  {
    "start": "node -r esm test/socket.test.js",

       "st": "eslint .",
     "unit": "test/run",
       "ts": "dtslint --expectOnly --localTs ./node_modules/typescript/lib",

     "test": "npm run st && npm run unit && npm run ts",

    "build": "gulp --final --to npm"
  },

  "dependencies":
  {
    "@streetstrider/emitter":
      "^1.2",

    "isomorphic-ws":
      "4 || 5",

    "aux.js":
      "^3.6"
  },

  "devDependencies":
  {
    "booth":
      "./",

    "esm":
      "3",

    "console-ultimate":
      "3",

    "ws":
      "8",


    "gulp":
      "4",

    "rollup":
      "2",

    "metalpipe":
      "https://github.com/StrangeTransistor/metalpipe.git#e335123b083b4cf67ac73ca12ef2f14632c8e6fe",


    "eslint":
      "8",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.11.0",

    "eslint-plugin-node":
      "11",


    "dtslint":
      "4.2",

    "typescript":
      "4.7.4",

    "@types/node":
      "18",

    "@types/ws":
      "^8.2",


    "chai":
      "4"
  }
}