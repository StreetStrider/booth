{
     "name": "booth",
  "version": "0.10.0",
  "license": "ISC",

  "author": "Strider <strange.transistor@gmail.com>",
  "description": "Booth provides request-response over socket.io and creates FRP streams from socket.io events",

  "homepage":            "https://github.com/StreetStrider/booth",
  "repository": { "url": "https://github.com/StreetStrider/booth.git", "type": "git" },
  "bugs":       { "url": "https://github.com/StreetStrider/booth/issues" },

  "engines":
  {
    "node": ">=6"
  },

  "main": "Booth",

  "files":
  [
    "Booth.js",
    "Endpoint.js",
    "lib",
    "dist",
    "flow-typed"
  ],

  "peerDependencies":
  {
    "socket.io":
      "2"
  },

  "dependencies":
  {
    "bluebird":
      "3.5",

    "most":
      "1.2"
  },

  "scripts":
  {
    "start": "npm run dev",

    "dev": "metalbox p dev",
    "npm": "metalbox p npm",

    "st":
      "eslint .",

    "flow":
      "flow check",

    "unit":
      "mocha --recursive release/dev/dist/test/case/",

    "test":
      "npm run st && npm run flow && npm run dev -- -1 && npm run unit"
  },

  "metalbox":
  {
    "presets":
    {
      "npm": [ "Library" ],
      "dev": [ "Library/Dev" ]
    }
  },

  "devDependencies":
  {
    "socket.io":
      "2",

    "socket.io-client":
      "2",


    "metalbox":
      "1.16",


    "eslint":
      "3",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.1.1",


    "mocha":
      "3",

    "chai":
      "3",


    "flow-bin":
      "0.54",

    "flow-typed":
      "2"
  }
}
